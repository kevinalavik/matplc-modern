/* note: this is the same file as io/comedi_syntax.dot, except that the
 * typeR node has been split into two, so as to split the diagram onto two
 * lines (the second typeR starts at rank 1 again)
 */

digraph G {

  node [width=0, height=0]

  rankdir=LR

  S -> device [minlen=0]
  device -> colon
  colon -> typeL [constraint=false]
  typeR2 -> subdevice -> dot -> channel -> options
  options -> E [minlen=2]

  S -> typeL
  typeR2 -> dot
  subdevice -> options

  typeR -> typeR2 [constraint=false]

  S [shape = doublecircle]
  E [shape = doublecircle]

  typeL [shape = doublecircle, label=""]
  typeR [shape = doublecircle, label=""]
  typeR2 [shape = doublecircle, label=""]
  options [shape = doublecircle, label=""]

  colon [shape = box, label=":"]
  dot [shape = box, label="."]
  under [shape = box, label="_"]
  slash [shape = box, label="/"]

  I [shape = box]
  O [shape = box]
  Q [shape = box]
  N [shape = box]
  U [shape = box]
  X [shape = box]
  Y [shape = box]

  typeL -> I -> typeR
  typeL -> O -> typeR
  typeL -> Q -> typeR
  typeL -> N -> typeR
  typeL -> U -> typeR
  typeL -> X -> typeR
  typeL -> Y -> typeR

  options -> under -> aref -> options
  options -> slash -> range -> options

}
